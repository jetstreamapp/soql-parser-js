  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "8.0.14",
    "antlr4ts-cli": "0.4.0-alpha.4",
    "awesome-typescript-loader": "^5.2.0",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
