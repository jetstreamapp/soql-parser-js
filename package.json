{
  "name": "soql-parser-js",
  "version": "1.0.0",
  "description": "",
  "main": "/build/app/index.js",
  "scripts": {
    "clean": "rm -rf ./build/*",
    "prestart": "npm run build",
    "start": "node build/app/index.js",
    "test": "mocha -r ts-node/register src/tests/**/*.spec.ts",
    "prebuild2": "npm run antlr; npm run clean",
    "prebuild": "npm run clean",
    "build": "npm run tsc",
    "tsc": "./node_modules/.bin/tsc",
    "preantlr": "rm -rf src/generated/*",
    "antlr": "./node_modules/.bin/antlr4ts -visitor src/SOQL.g4 -o src/generated/",
    "package": "npm run package:clean && npm run package:build && npm run package:ems && npm run package:umd",
    "package:pre-build": "DO STUFF HERE - like build antlr etc.. - might not need this",
    "package:clean": "rm -rf ./lib ./lib-esm _bundles",
    "package:build": "tsc -p tsconfig.package.json",
    "package:build:ems": "tsc -p tsconfig.package.json -m es6 --outDir lib-esm",
    "package:build:umd": "webpack"
  },
